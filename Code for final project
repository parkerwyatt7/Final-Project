import turtle
wn = turtle.Screen()
bob = turtle.Turtle()
def animal():
    animal = input("What animal do you like?")
    if animal == "dog":
        bob.circle(100,360)
        bob.penup()
        bob.goto(0,70)
        bob.right(90)
        bob.pendown()
        bob.fd(20)
        bob.penup()
        bob.goto(20,70)
        bob.pendown()
        bob.fd(30)
        for i in range(18):
            bob.fd(3)
            bob.right(10)
        bob.fd(30)
        bob.penup()
        bob.goto(0,70)
        bob.pendown()
        bob.right(45)
        bob.fd(10)
        for i in range(9):
            bob.fd(2)
            bob.left(5)
        bob.left(45)
        for i in range(9):
            bob.fd(2)
            bob.left(5)
        for i in range(9):
            bob.fd(2)
            bob.left(5)
        bob.penup()
        bob.goto(0,70)
        bob.right(90)
        bob.pendown()
        bob.fd(10)
        for i in range(9):
            bob.fd(2.5)
            bob.right(5)
        bob.penup()
        bob.goto(-60,125)
        bob.pendown()
        for i in range(18):
            bob.fd(4)
            bob.right(10)
        bob.penup()
        bob.goto(20,125)
        bob.right(180)
        bob.pendown()
        for i in range(18):
            bob.fd(4)
            bob.right(10)
        bob.penup()
        bob.goto(-60,177)
        bob.pendown()
        bob.left(180)
        bob.left(45)
        for i in range(9):
            bob.fd(15)
            bob.left(10)
        bob.right(45)
        bob.penup()
        bob.goto(60,177)
        bob.pendown()
        bob.right(180)
        for i in range(9):
            bob.fd(15)
            bob.right(10)
