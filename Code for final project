            bob.fd(3)
            bob.left(10)
        for i in range(9):
            bob.fd(3)
            bob.right(10)
        bob.left(180)
        bob.fd(70)
        bob.penup()
        bob.goto(-35,130)
        bob.right(90)
        bob.pendown()
        for i in range(18):
            bob.fd(2)
            bob.left(10)
        bob.fd(20)
        for i in range(18):
            bob.fd(2)
            bob.left(10)
